CREATE TABLE CLIENTE (
	CPF INTEGER NOT NULL,
	NOME VARCHAR (50 ) NOT NULL,
	RUA VARCHAR (50 ),
	NUMERO INTEGER,
	BAIRRO VARCHAR (50 )
);

//INSERT INTO CLIENTE(CPF, NOME, RUA, NUMERO, BAIRRO)
//VALUES(45567, 'Gabriel', 'Laele', 153, 'Laelandia');

CREATE TABLE FUNCIONARIOS(
	CPF INTEGER NOT NULL,
	NOME VARCHAR (50 ) NOT NULL,
	CARGO VARCHAR (50 ) NOT NULL,
	PROJETOS VARCHAR (50 )
);

SELECT * FROM FUNCIONARIOS;